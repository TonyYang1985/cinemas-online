{
    "addNewline": true,
    "excludes": [
      "@types",
      "typings",
      "__test__",
      "__tests__",
      "node_modules",
      "tools"
    ],
    "fileExcludePatterns": [],
    "fileFirst": false,
    "globOptions": {
      "dot": true,
      "ignore": [
        "@types/**",
        "typings/**",
        "__test__/**",
        "__tests__/**",
        "node_modules/**",
        "tools/**"
      ],
      "nonull": true
    },
    "quote": "'",
    "targetExts": [
      "ts",
      "tsx"
    ],
    "useSemicolon": true,
    "useTimestamp": false,
    "verbose": true,
    "withoutBackupFile": true,
    "withoutComment": false,
    "includeCWD": true,
    "output": "index.ts"
  }