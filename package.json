  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "//===build===": "",
    "ncc:build": "ncc build ./src/app.ts -m -o ./dist/single -e sqlite3 --no-source-map-register && ncc cache clean",
    "ncc:run": "cross-env NODE_ENV=development ncc run ./src/app.ts",
    "buildNum": "yarn json -I -f package.json -e \"this.buildNumber=$(yarn -s buildnumgen)\"",
    "//===local start===": "",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:dev:debug": "cross-env NODE_ENV=development nodemon --inspect",
    "start:prod": "cross-env ENABLE_API_GATEWAY=true API_GATEWAY_HOST_PORT=192.168.52.35:80 DOMAINS=foo.sg,*.foo.sg NODE_ENV=production tsc-watch --onSuccess \"node -r tsconfig-paths/register -r source-map-support/register ./temp/example/app.js\"",
    "//=== gen  schema repository ===": "",
    "gen:db-schema": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register tools/DBSchemaGenerator.ts",
    "gen:db-repo": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register tools/RepositoryGenerator.ts",
    "gen:db": "yarn gen:db-schema && yarn gen:db-repo && yarn lint:fix",
    "gen:keys": "openssl genrsa -out ./keys/privateKey.pem 2048 && openssl rsa -in ./keys/privateKey.pem -outform PEM -pubout -out ./keys/publicKey.pem",
    "//=== jest ===": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "//=== eslint===": "",
    "lint": "tsc --noEmit && eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "//=== vulnerability===": "",
    "security": "yarn audit",
    "security:summary": "yarn audit --summary",
    "security:json": "yarn audit --json",
    "//=== npm check update  ===": "",
    "deps:check": "ncu",
    "deps:update": "ncu -u && yarn install",
    "precommit": "yarn lint"
  },
